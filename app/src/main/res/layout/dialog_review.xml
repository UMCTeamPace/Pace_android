<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="300dp"
    android:layout_height="311dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/box_dialog">
    <View
        android:id="@+id/review_app_icon"
        android:layout_width="104dp"
        android:layout_height="103dp"
        android:background="#F7F5EB"
        android:layout_marginTop="34dp"
        android:layout_marginStart="26dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/review_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="잘 즐기고 계신가요?"
        android:textAppearance="@style/TextAppearance.App.Num.BodyMd.Medium"
        android:layout_marginTop="16dp"
        android:layout_marginStart="28dp"
        app:layout_constraintTop_toBottomOf="@id/review_app_icon"
        app:layout_constraintStart_toStartOf="parent" />
    <TextView
        android:id="@+id/review_rate_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="☆ ☆ ☆ ☆ ☆"
        android:textSize="38dp"
        android:fontFamily="@font/roboto_medium"
        android:layout_marginTop="12dp"
        android:layout_marginStart="31dp"
        app:layout_constraintTop_toBottomOf="@id/review_tv"
        app:layout_constraintStart_toStartOf="parent"/>
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/review_cancel_btn"
        android:layout_width="258dp"
        android:layout_height="38dp"
        android:text="지금 안 할래요"
        android:layout_marginTop="13dp"
        android:layout_marginStart="21dp"
        android:layout_marginEnd="21dp"
        android:background="@drawable/btn_dialog"
        android:stateListAnimator="@null"
        app:layout_constraintTop_toBottomOf="@id/review_rate_tv"
        app:layout_constraintStart_toStartOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>